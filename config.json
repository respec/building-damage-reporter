{
  "data": {
    "table": "mapfeeder",
    "fields": [
      "id",
      "strftime('%Y-%m-%d %H:%M',datetime(timestamp, 'localtime')) AS Timestamp",
      "issue_type AS 'Issue Type'",
      "issue_city AS 'City'",
      "issue_state AS 'State'",
      "uploads AS Photos",
      "lat || ', ' || lng AS Location",
      "lng AS x",
      "lat AS y"
    ]
  },
